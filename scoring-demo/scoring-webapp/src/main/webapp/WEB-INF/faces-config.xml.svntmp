<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">
	<application>
		<message-bundle>locale.messages</message-bundle>
		<locale-config>
			<default-locale>en</default-locale>
		</locale-config>
		<resource-bundle>
			<base-name>locale.messages</base-name>
			<var>msg</var>
		</resource-bundle>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
	</application>
	
	
	<managed-bean>
		<managed-bean-name>scoringSchemeBean</managed-bean-name>
		<managed-bean-class>com.demo.jsf.web.mb.ScoringSchemeBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>schemeService</property-name>
			<value>#{schemeService}</value>
		</managed-property>
		<managed-property>
			<property-name>ruleService</property-name>
			<value>#{ruleService}</value>
		</managed-property>
		<managed-property>
			<property-name>ruleCaseService</property-name>
			<value>#{ruleCaseService}</value>
		</managed-property>
		<managed-property>
			<property-name>factorService</property-name>
			<value>#{factorService}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>scoringFactorBean</managed-bean-name>
		<managed-bean-class>com.demo.jsf.web.mb.ScoringFactorBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>factorService</property-name>
			<value>#{factorService}</value>
		</managed-property>
		<managed-property>
			<property-name>ruleService</property-name>
			<value>#{ruleService}</value>
		</managed-property>
	</managed-bean>
	
	
	<managed-bean>
		<managed-bean-name>scoringEvaluationBean</managed-bean-name>
		<managed-bean-class>com.demo.jsf.web.mb.ScoringEvaluationBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>schemeService</property-name>
			<value>#{schemeService}</value>
		</managed-property>
		<managed-property>
			<property-name>ruleService</property-name>
			<value>#{ruleService}</value>
		</managed-property>
		<managed-property>
			<property-name>ruleCaseService</property-name>
			<value>#{ruleCaseService}</value>
		</managed-property>
		<managed-property>
			<property-name>factorService</property-name>
			<value>#{factorService}</value>
		</managed-property>
		<managed-property>
			<property-name>mappingSchemeService</property-name>
			<value>#{mappingSchemeService}</value>
		</managed-property>
		<managed-property>
			<property-name>mappingRuleService</property-name>
			<value>#{mappingRuleService}</value>
		</managed-property>
		<managed-property>
			<property-name>mappingRuleCaseService</property-name>
			<value>#{mappingRuleCaseService}</value>
		</managed-property>
		<managed-property>
			<property-name>appFieldSpecService</property-name>
			<value>#{appFieldSpecService}</value>
		</managed-property>
		<managed-property>
			<property-name>appSpecsService</property-name>
			<value>#{appSpecsService}</value>
		</managed-property>
		<managed-property>
			<property-name>extSystemService</property-name>
			<value>#{extSystemService}</value>
		</managed-property>
		
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>externalSystemBean</managed-bean-name>
		<managed-bean-class>com.demo.jsf.web.mb.ExternalSystemBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>extSystemService</property-name>
			<value>#{extSystemService}</value>
		</managed-property>
		<managed-property>
			<property-name>appSpecsService</property-name>
			<value>#{appSpecsService}</value>
		</managed-property>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>appSpecsBean</managed-bean-name>
		<managed-bean-class>com.demo.jsf.web.mb.ApplicationSpecificationBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>appSpecsService</property-name>
			<value>#{appSpecsService}</value>
		</managed-property>
		<managed-property>
			<property-name>appFieldSpecService</property-name>
			<value>#{appFieldSpecService}</value>
		</managed-property>
		<managed-property>
			<property-name>dataTypeService</property-name>
			<value>#{dataTypeService}</value>
		</managed-property>
		<managed-property>
			<property-name>ruleService</property-name>
			<value>#{ruleService}</value>
		</managed-property>
		<managed-property>
			<property-name>mappingSchemeService</property-name>
			<value>#{mappingSchemeService}</value>
		</managed-property>
		<managed-property>
			<property-name>extSystemService</property-name>
			<value>#{extSystemService}</value>
		</managed-property>
		
		
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>mappingSchemeBean</managed-bean-name>
		<managed-bean-class>com.demo.jsf.web.mb.MappingSchemeBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>mappingSchemeService</property-name>
			<value>#{mappingSchemeService}</value>
		</managed-property>
		<managed-property>
			<property-name>mappingRuleService</property-name>
			<value>#{mappingRuleService}</value>
		</managed-property>
		
		<managed-property>
			<property-name>schemeService</property-name>
			<value>#{schemeService}</value>
		</managed-property>
		<managed-property>
			<property-name>ruleService</property-name>
			<value>#{ruleService}</value>
		</managed-property>
		<managed-property>
			<property-name>ruleCaseService</property-name>
			<value>#{ruleCaseService}</value>
		</managed-property>
		<managed-property>
			<property-name>mappingRuleCaseService</property-name>
			<value>#{mappingRuleCaseService}</value>
		</managed-property>
		<managed-property>
			<property-name>appSpecsService</property-name>
			<value>#{appSpecsService}</value>
		</managed-property>
		
	</managed-bean>
	
</faces-config>
